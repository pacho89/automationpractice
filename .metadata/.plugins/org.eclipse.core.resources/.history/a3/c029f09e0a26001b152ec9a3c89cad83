import cucumber.api.java.en.Given;
import cucumber.api.java.en.Then;
import cucumber.api.java.en.When;

public class SteDefinitionCP_009 {
	
	@Given("^I am in sign in page$")
	public void i_am_in_sign_in_page() throws Throwable {
	    System.out.println("Open page sign in");
	}
	

	@When("^I enter email \"([^\"]*)\"$")
	public void i_enter_email(String arg1) throws Throwable {
	    System.out.println("Type email");
	}

	@When("^I press Create an account$")
	public void i_press_Create_an_account() throws Throwable {
	    System.out.println("Press Create an account");
	}

	@Then("^Redirect to account-creation page$")
	public void redirect_to_account_creation_page() throws Throwable {
	    System.out.println("Open page account creation");
	}

	@Then("^check Mrs$")
	public void check_Mrs() throws Throwable {
		System.out.println("Check box Mrs");
	}

	@Then("^fill in \"([^\"]*)\" with \"([^\"]*)\"$")
	public void fill_in_with(String arg1, String arg2) throws Throwable {
		System.out.println("Type Firts Name and Last Name");
	}

	@Then("^Select in \"([^\"]*)\" with \"([^\"]*)\"$")
	public void select_in_with(String arg1, String arg2) throws Throwable {
	    System.out.println("Type Email and Password");
	}

	}

	@Then("^I should see \"([^\"]*)\"$")
	public void i_should_see(String arg1) throws Throwable {
	    System.out.println("Open page of My account");
	}



}

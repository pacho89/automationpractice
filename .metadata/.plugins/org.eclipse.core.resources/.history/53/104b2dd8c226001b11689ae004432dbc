import cucumber.api.java.en.Then;
import cucumber.api.java.en.When;

public class DefinitionCP_012 {


	@When("^I press CREATE AN ACCOUNT$")
	public void i_press_CREATE_AN_ACCOUNT() throws Throwable {
	    System.out.println("Press button create account");
	}

	@Then("^I press Create an Account$")
	public void i_press_Create_an_Account() throws Throwable {
		System.out.println("Press button create account");
	}

	@Then("^I press Register$")
	public void i_press_Register() throws Throwable {
		System.out.println("Press Resgiter");
	}

	@Then("^Show invalid message There are (\\d+) errors$")
	public void show_invalid_message_There_are_errors(int arg1) throws Throwable {
		System.out.println("Show invalid message");
	}

}
